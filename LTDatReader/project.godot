config_version=4

[application]

config/name="LithTools"
run/main_scene="res://Scenes/Default.tscn"
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[autoload]

Globals="*res://UI/Src/Globals.gd"
LoadedFile="*res://UI/Src/LoadedFile.gd"
Events="*res://UI/Src/Events.gd"

[display]

window/size/width=1280
window/size/height=720

[editor_plugins]

enabled=PoolStringArray( "ABCReader", "DTXReader", "LTDatReader", "godot-plugin-refresher" )

